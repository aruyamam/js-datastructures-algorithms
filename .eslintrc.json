{
   "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
   },
   "rules": {
      "indent": [
         "error",
         3,
         {
            "SwitchCase": 1
         }
      ],
      "brace-style": [
         "error",
         "stroustrup"
      ],
      "no-plusplus": "off"
   },
   "parser": "babel-eslint",
   "extends": "airbnb-base"
}
